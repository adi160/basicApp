<div class = "page-header">
  <h1> SignUp page </h1>
</div>
<div class = "form">
  <div class = "well">
    <%= bootstrap_form_for :user, url:users_path, class: "signupForm" do |f| %>
      <%= f.text_field :first_name %>
      <%= f.text_field :last_name %>
      <%= f.text_field :email %>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation %> 
      <%= f.submit "Create account", :class => "btn btn-default" %></p>
    <% end %>
  </div>
</div>